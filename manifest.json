{
  "manifest_version": 3,
  "name": "Bustem",
  "version": "1.0",
  "description": "Blocks cryptojacking",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "system.cpu"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "CryptoBlock"
  },
  "web_accessible_resources": [
    {
      "resources": ["blockpage.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
